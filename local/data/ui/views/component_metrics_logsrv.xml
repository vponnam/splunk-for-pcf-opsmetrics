<?xml version="1.0"?><form>
  <label>Doppler Server</label>
  <fieldset autoRun="true" submitButton="false">
    <input type="dropdown" token="prod" searchWhenChanged="true">
      <label>Choose PCF Deployment</label>
      <choice value="jmx_prod_*">All</choice>
      <choice value="jmx_prod_east">Azure Prod East</choice>
      <choice value="jmx_prod_west">Azure Prod West</choice>
      <default>jmx_prod_*</default>
      <initialValue>jmx_prod_*</initialValue>
    </input>
    <input type="time" token="time" searchWhenChanged="true">
      <label>Time Range</label>
      <default>
        <earliest>-15m</earliest>
        <latest>now</latest>
      </default>
    </input>
  </fieldset>
  <searchTemplate>index=$prod$
  host="*"
mbean_domain="org.cloudfoundry"
mbean_property_deployment="cf-deployment"
mbean_property_job="DopplerServer"
    | eval job_and_index = jvmDescription + "_" + mbean_property_job + "_" + mbean_property_index
    | fields job_and_index
      DopplerServer.messageRouter.numberOfDumpSinks
      DopplerServer.messageRouter.numberOfSyslogSinks
      DopplerServer.messageRouter.numberOfWebsocketSinks
      DopplerServer.httpServer.numberOfMessagesDroppedInParseEnvelopes
      DopplerServer.httpServer.numberOfMessagesUnmarshalErrorsInParseEnvelopes
      DopplerServer.httpServer.numberOfMessagesUnmarshalledInParseEnvelopes
      DopplerServer.numGoRoutines
      DopplerServer.memoryStats.lastGCPauseTimeNS
      DopplerServer.dropsondeListener.currentBufferCount
      DopplerServer.dropsondeListener.receivedByteCount
      DopplerServer.dropsondeListener.receivedMessageCount</searchTemplate>

    <earliestTime>$time.earliest$</earliestTime>
  <latestTime>$time.latest$</latestTime>  
<row>
    <chart>
      <title>Agent Listener Stats</title>
      <searchPostProcess>timechart avg(DopplerServer.dropsondeListener.currentBufferCount) as "Buffer Count" avg(DopplerServer.dropsondeListener.receivedByteCount) as "Rx Bytes" avg(DopplerServer.dropsondeListener.receivedMessageCount) as "Rx Messages" by job_and_index useother=f limit=200</searchPostProcess>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.axisTitleX.text">Time</option>
      <option name="charting.axisTitleY.text">Count</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <chart>
      <title>GC Pause</title>
      <searchPostProcess>rename DopplerServer.memoryStats.lastGCPauseTimeNS as GCPause | eval GCPause = GCPause/100000 | timechart avg(GCPause) as "GC Pause" by job_and_index useother=f limit=200</searchPostProcess>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">10</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">1</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="rowNumbers">0</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">1</option>
      <option name="type">list</option>
      <option name="charting.axisTitleY.text">Ms</option>
      <option name="charting.axisTitleX.text">Time</option>
    </chart>
  </row>
  <row>
    <chart>
      <title>Go Routines</title>
      <searchPostProcess>timechart avg(DopplerServer.numGoRoutines) as "Go Routines" by job_and_index useother=f limit=200</searchPostProcess>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">10</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">1</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="rowNumbers">0</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">1</option>
      <option name="type">list</option>
      <option name="charting.axisTitleY.text">Count</option>
      <option name="charting.axisTitleX.text">Time</option>
    </chart>
  </row>
  <row>
    <chart>
      <title>Http Server</title>
      <searchPostProcess>rename DopplerServer.httpServer.numberOfMessagesDroppedInParseEnvelopes as numberOfMessagesDroppedInParseEnvelopes 
       | rename DopplerServer.httpServer.numberOfMessagesUnmarshalErrorsInParseEnvelopes as numberOfMessagesUnmarshalErrorsInParseEnvelopes
       | timechart avg(numberOfMessagesDroppedInParseEnvelopes) as "Drop Mx " avg( ) as "Mx Errors" avg(numberOfMessagesUnmarshalledInParseEnvelopes) as "Mx UnMarsh" by job_and_index useother=f limit=200</searchPostProcess>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">10</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">1</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="rowNumbers">0</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">1</option>
      <option name="type">list</option>
      <option name="charting.axisTitleY.text">Messages</option>
      <option name="charting.axisTitleX.text">Time</option>
    </chart>
  </row>
  <row>
    <chart>
      <title>Message Router</title>
      <searchPostProcess>timechart avg(DopplerServer.messageRouter.numberOfDumpSinks) as "Dump" avg(DopplerServer.messageRouter.numberOfSyslogSinks) as "Syslog" avg( DopplerServer.messageRouter.numberOfWebsocketSinks) as "Socket" by job_and_index useother=f limit=200</searchPostProcess>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">10</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">1</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="rowNumbers">0</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">1</option>
      <option name="type">list</option>
      <option name="charting.axisTitleY.text">Sinks</option>
      <option name="charting.axisTitleX.text">Time</option>
    </chart>
  </row>
</form>