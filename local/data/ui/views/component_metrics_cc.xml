<?xml version="1.0"?><form>
  <label>CloudController</label>
  <fieldset autoRun="true">
    <input type="dropdown" token="jvmDescription">
      <label>Choose JVM Description</label>
      <prefix>jvmDescription="</prefix>
      <suffix>"</suffix>
      <choice value="*">All</choice>
      <default>*</default>
      <populatingSearch fieldForValue="jvmDescription" fieldForLabel="jvmDescription" earliest="-1d@d" latest="now">
        <![CDATA[index=main sourcetype=jmx | dedup jvmDescription | table jvmDescription]]>
      </populatingSearch>
    </input>
    <input type="time">
      <default>
        <earliestTime>rt-1h</earliestTime>
        <latestTime>rt</latestTime>
      </default>
    </input>
  </fieldset>
  <searchTemplate>index=main sourcetype=jmx host="*" jvmDescription mbean_domain="org.cloudfoundry" mbean_property_deployment="untitled_dev" mbean_property_job="CloudController" | eval job_and_index = jvmDescription + "_" + mbean_property_job + "_" + mbean_property_index | fields job_and_index cc.http_status.1XX cc.http_status.2XX cc.http_status.3XX cc.http_status.4XX cc.http_status.5XX cc.requests.outstanding cc.requests.completed cc.job_queue_length.cc-generic total_users cpu_load_avg  "log_count[level=warn]" "log_count[level=error]" "log_count[level=fatal]" cc.thread_info.thread_count cc.thread_info.event_machine.connection_count cc.thread_info.event_machine.resultqueue.size cc.thread_info.event_machine.resultqueue.num_waiting cc.thread_info.event_machine.threadqueue_waiting g cc.thread_info.event_machine.threadqueue.size</searchTemplate>
  <row>
    <chart>
      <title>HTTP Status Count</title>
      <searchPostProcess>timechart avg(cc.http_status.1XX) as "HTTP 1XX  Count" avg(cc.http_status.2XX) as "HTTP 2XX Count" avg(cc.http_status.3XX) as "HTTP 3XX Count" avg(cc.http_status.4XX) as "HTTP 4XX Count" avg(cc.http_status.5XX) as "HTTP 5XX Count" by job_and_index useother=f limit=200</searchPostProcess>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.axisTitleX.text">Time</option>
      <option name="charting.axisTitleY.text">Count</option>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
    </chart>
  </row>
  <row>
    <chart>
      <title>Requests</title>
     <searchPostProcess>timechart avg(cc.requests.outstanding) as "Outstanding Requests" avg(cc.requests.completed) as "Completed Requests" by job_and_index useother=f limit=200</searchPostProcess>
      <earliestTime>$earliest$</earliestTime>
      <latestTime>$latest$</latestTime>
      <option name="charting.axisTitleX.visibility">visible</option>
      <option name="charting.axisTitleY.visibility">visible</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">connect</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">default</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">right</option>
      <option name="count">10</option>
      <option name="list.drilldown">full</option>
      <option name="list.wrap">1</option>
      <option name="maxLines">5</option>
      <option name="raw.drilldown">full</option>
      <option name="rowNumbers">0</option>
      <option name="table.drilldown">1</option>
      <option name="table.wrap">1</option>
      <option name="type">list</option>
      <option name="charting.axisTitleY.text">Requests</option>
      <option name="charting.axisTitleX.text">Time</option>
    </chart>
  </row>
  <row>
