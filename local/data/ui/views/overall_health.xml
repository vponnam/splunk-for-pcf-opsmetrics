    <panel>
      <title>Router Request per Second</title>
      <single>
        <title>Router Req/s (5m)</title>
        <search>
          <query>index=$prod$ host="*"  router.requests_per_sec = * jvmDescription mbean_domain="org.cloudfoundry" mbean_property_deployment="cf-*" mbean_property_job="router*" earliest=-5m latest=now| eval job_and_index = jvmDescription + "_" + mbean_property_job + "_" + mbean_property_index + "_" + mbean_property_ip | rename "router.total_requests" as total_requests | stats avg(total_requests) as sum_total_requests |eval sum=(sum_total_requests)/300 | table sum</query>
