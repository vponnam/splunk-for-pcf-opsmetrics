<form>
  <label>Overall Health</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="radio" token="prod">
      <label>Environment</label>
      <choice value="jmx_prod_east">Azure Prod East</choice>
      <choice value="jmx_prod_west">Azure Prod West</choice>
    </input>
  </fieldset>
  <row>
    <panel> 
     <title>Frontend Health</title>
      <single>
        <title>Router_Partition_24h</title>
        <search>
          <query>index=$prod$  host="*" jvmDescription mbean_domain="org.cloudfoundry" mbean_property_deployment="cf-*" mbean_property_job="router*"  earliest=-1d latest=now() | eval Date=if(_time=relative_time(now(),"-1d"), "Latest","Earliest") | bucket _time span=1d | stats avg(system.healthy) as system by _time | eval average=system*100 | table average</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
          <option name="height">10</option>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
